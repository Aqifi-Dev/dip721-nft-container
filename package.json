{
  "name": "hello_frontend",
  "version": "0.1.0",
  "description": "Internet Computer starter application",
  "keywords": [
    "Internet Computer",
    "Rust",
    "JavaScript",
    "Canister"
  ],
  "scripts": {
    "type": "module",
    "build": "webpack",
    "prebuild": "npm run generate",
    "start": "webpack serve --mode development --env development",
    "prestart": "npm run generate",
    "generate": "dfx generate hello_frontend",
    "g1": "dfx generate dip721_nft_container",
    "g2": "dfx generate hello",
    "g3": "dfx generate hello_frontend",
    "installall": "dfx canister install --all --mode auto",
    "reinstallfrontend": "dfx canister install hello_frontend --mode reinstall",
    "dstop": "dfx stop",
    "d0c": "dfx start --background --clean",
    "d0d": "dfx start --background",
    "once": "./bashscript.sh once",
    "d01": "./bashscript.sh cargocheck",
    "d02": "./bashscript.sh useDfxAdmin",
    "d0": "./bashscript.sh startDfx",
    "d1": "./bashscript.sh deployHello local",
    "d2": "./bashscript.sh deployDip721 local",
    "d2a": "./bashscript.sh deployDip721a local",
    "d2nl": "./bashscript.sh deployDip721no_logo local",
    "d3": "./bashscript.sh deployHelloFrontend local",
    "d4": "./bashscript.sh callSimpleFunctions local",
    "d5": "./bashscript.sh callInterCanister local",
    "d6": "./bashscript.sh mintNFT KingKong local",
    "d6g": "./bashscript.sh getMetadata 0 local",
    "d6s": "./bashscript.sh setMetadata 0 local",
    "d6a": "./bashscript.sh mintDip721forall AnyBody local",
    "d6ag": "./bashscript.sh getMetadata 1 local",
    "d6zz": "./bashscript.sh mint_old_method local",
    "d7": "./bashscript.sh getBalances local",
    "d8": "./bashscript.sh transfer local",
    "d9": "./bashscript.sh approve local",
    "d90": "./bashscript.sh setcycles ic 1",
    "d91": "./bashscript.sh deployHello ic",
    "d92": "./bashscript.sh deployDip721 ic",
    "d93": "./bashscript.sh deployHelloFrontend ic",
    "d94": "./bashscript.sh callSimpleFunctions ic",
    "d95": "./bashscript.sh callInterCanister ic",
    "d96": "./bashscript.sh mintNFT KingKong ic",
    "d96g": "./bashscript.sh getMetadata 0 ic",
    "d96a": "./bashscript.sh mintDip721forall AnyBody ic",
    "d96ag": "./bashscript.sh getMetadata 1 ic",

    "d11": "node --es-module-specifier-resolution=node src/node/index11.js",
    "d11a": "node --es-module-specifier-resolution=node src/node/index11a.js",
    "d12": "node --es-module-specifier-resolution=node src/node/index12.js",
    "d21": "dfx identity get-principal",
    "d22": "dfx ledger --network ic account-id"
  },
  "dependencies": {
    "@dfinity/identity-secp256k1": "^0.15.1",
    "@slide-computer/assets": "^1.3.0",
    "@types/node": "^18.11.18",
    "dotenv": "^16.0.3",
    "ic0": "^0.2.3",
    "image-thumbnail": "^1.0.15",
    "isomorphic-fetch": "^3.0.0",
    "mmmagic": "^0.5.3",
    "prettier": "^2.8.2",
    "sha256-file": "^1.0.0",
    "ts-node": "^10.9.1",
    "tweetnacl": "^1.0.3",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@dfinity/agent": "^0.15.1",
    "@dfinity/candid": "^0.15.1",
    "@dfinity/principal": "^0.15.1",
    "@types/image-thumbnail": "^1.0.1",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/mmmagic": "^0.4.30",
    "@types/prettier": "^2.7.2",
    "@types/sha256-file": "^1.0.1",
    "assert": "2.0.0",
    "buffer": "6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "events": "3.3.0",
    "html-webpack-plugin": "5.5.0",
    "process": "0.11.10",
    "stream-browserify": "3.0.0",
    "terser-webpack-plugin": "^5.3.3",
    "util": "0.12.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "engines": {
    "node": " ^18 || ^19"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ]
}
